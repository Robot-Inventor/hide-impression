{
    "manifest_version": 2,
    "name": "Hide View Count",
    "short_name": "HideViewCount",
    "version": "1.0.0",
    "description": "__MSG_manifest_description__",
    "icons": {
        "16": "image/icon16.png",
        "48": "image/icon48.png",
        "128": "image/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/*",
                "https://mobile.twitter.com/*",
                "https://tweetdeck.twitter.com/*"
            ],
            "js": [
                "js/index.js"
            ]
        }
    ],
    "default_locale": "en",
    "browser_specific_settings": {
        "gecko_android": {}
    }
}